            <nonFilteredFileExtension>glb</nonFilteredFileExtension>
            <nonFilteredFileExtension>wasm</nonFilteredFileExtension>
            <nonFilteredFileExtension>ico</nonFilteredFileExtension>
          </nonFilteredFileExtensions>
        </configuration>
      </plugin>
    </plugins>
    <resources>
